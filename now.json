{
    "version": 2,
    "name": "fufefe",
    "builds": [
        { "src": "package.json", "use": "@now/static-build" },
        { "src": "proxy/*.php", "use": "@now/php" }
    ],
    "routes": [
        { "src": "/proxy/(.*)", "dest": "/proxy/$1" },
        { "src": "/index.html", "dest": "/public/index.html" },
        { "src": "/style.min.css", "dest": "/public/style.min.css" },
        { "src": "/script.min.js", "dest": "/public/script.min.js" }
    ]
}