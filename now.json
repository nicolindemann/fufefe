{
    "version": 2,
    "name": "fufefe",
    "builds": [
        { "src": "www/package.json", "use": "@now/static-build" },
        { "src": "proxy/*.js", "use": "@now/node" }
    ],
    "routes": [
        { 
            "src": "/proxy/(.*)", 
            "dest": "/proxy/$1" 
        },
		{
			"src": "/(.*)",
			"dest": "/www/$1"
		}
    ],
    "alias": ["fufefe"]
}